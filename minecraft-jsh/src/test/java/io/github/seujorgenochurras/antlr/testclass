package io.github.seujorgenochurras.antlr;

import io.github.seujorgenochurras.minecraftjsh.antlr.lexer.io.github.seujorgenochurras.minecraftjsh.antlr.lexer.JavaLexer;
import io.github.seujorgenochurras.minecraftjsh.antlr.parser.io.github.seujorgenochurras.minecraftjsh.antlr.parser.JavaParser;
import org.antlr.v4.runtime.CharStreams;
import org.antlr.v4.runtime.CommonTokenStream;

public class ParserTest {
    int a = j;
    void symbolNotFoundTest(){
        String code = "public class MeuPau {" +
                "int a = j;" +
                "}";
        io.github.seujorgenochurras.minecraftjsh.antlr.lexer.JavaLexer javaLexer = new io.github.seujorgenochurras.minecraftjsh.antlr.lexer.JavaLexer(CharStreams.fromString(code));
        CommonTokenStream commonTokenStream = new CommonTokenStream(javaLexer);

        io.github.seujorgenochurras.minecraftjsh.antlr.parser.JavaParser parser = new io.github.seujorgenochurras.minecraftjsh.antlr.parser.JavaParser(commonTokenStream);

    }
}
